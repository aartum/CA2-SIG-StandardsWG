#################################################################
#    Object Properties
#################################################################

###  http://purl.org/aiaontology#calculateHasResult
base:calculateHasResult rdf:type owl:ObjectProperty ;
                        rdfs:domain base:Calculating .


###  http://purl.org/aiaontology#calibratedBy
base:calibratedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf base:calibrates ;
                  rdfs:domain base:MeasurementInstrument ;
                  rdfs:range base:Calibrator .


###  http://purl.org/aiaontology#calibrates
base:calibrates rdf:type owl:ObjectProperty ;
                rdfs:domain base:Calibrator ;
                rdfs:range base:MeasurementInstrument .


###  http://purl.org/aiaontology#compliesWithRequirements
base:compliesWithRequirements rdf:type owl:ObjectProperty ;
                              rdfs:domain base:Auditor ;
                              rdfs:range base:Control .


###  http://purl.org/aiaontology#comprises
base:comprises rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ,
                        owl:ReflexiveProperty ;
               rdfs:comment "USE: For Activity, Control, Agent"@en .


###  http://purl.org/aiaontology#defines
base:defines rdf:type owl:ObjectProperty ;
             owl:inverseOf base:isDefinedBy ;
             rdfs:range base:Environment ;
             rdfs:comment "A collection of Parameters defines an Environment."@en .


###  http://purl.org/aiaontology#enacts
base:enacts rdf:type owl:ObjectProperty ;
            owl:inverseOf base:isEnactedBy ;
            rdfs:domain base:Agent ;
            rdfs:range base:Role .


###  http://purl.org/aiaontology#engagesIn
base:engagesIn rdf:type owl:ObjectProperty ;
               owl:inverseOf base:isPerformedBy ;
               rdfs:domain base:Agent ;
               rdfs:range base:Activity .


###  http://purl.org/aiaontology#forParameter
base:forParameter rdf:type owl:ObjectProperty ;
                  rdfs:range base:Parameter .


###  http://purl.org/aiaontology#hasActivity
base:hasActivity rdf:type owl:ObjectProperty ;
                 rdfs:range base:Activity .


###  http://purl.org/aiaontology#hasCalculation
base:hasCalculation rdf:type owl:ObjectProperty .


###  http://purl.org/aiaontology#hasClaimant
base:hasClaimant rdf:type owl:ObjectProperty ;
                 owl:inverseOf base:makesClaim ;
                 rdfs:domain base:Claim ;
                 rdfs:range base:Agent .


###  http://purl.org/aiaontology#hasCondition
base:hasCondition rdf:type owl:ObjectProperty ;
                  rdfs:domain base:Control ;
                  rdfs:range base:State ;
                  rdfs:comment "Indicates the State that must exist for the specific Control to be applicable."@en .


###  http://purl.org/aiaontology#hasControl
base:hasControl rdf:type owl:ObjectProperty ;
                rdfs:range base:Control .


###  http://purl.org/aiaontology#hasDuration
base:hasDuration rdf:type owl:ObjectProperty ;
                 rdfs:domain base:Period ;
                 rdfs:range <http://www.w3.org/2006/time#Duration> .


###  http://purl.org/aiaontology#hasEnvironment
base:hasEnvironment rdf:type owl:ObjectProperty ;
                    rdfs:domain base:State ;
                    rdfs:range base:Environment ;
                    rdfs:comment "The Environment for which the Parameter's value was measured/calculated"@en .


###  http://purl.org/aiaontology#hasFormula
base:hasFormula rdf:type owl:ObjectProperty .


###  http://purl.org/aiaontology#hasInstrument
base:hasInstrument rdf:type owl:ObjectProperty ;
                   rdfs:domain base:Activity ;
                   rdfs:range base:Instrument .


###  http://purl.org/aiaontology#hasLocation
base:hasLocation rdf:type owl:ObjectProperty ;
                 rdfs:range base:Location .


###  http://purl.org/aiaontology#hasMethodology
base:hasMethodology rdf:type owl:ObjectProperty ;
                    rdfs:range base:Control .


###  http://purl.org/aiaontology#hasParVal
base:hasParVal rdf:type owl:ObjectProperty ;
               owl:inverseOf base:isParValOf ;
               rdfs:domain base:Parameter ;
               rdfs:range base:State .


###  http://purl.org/aiaontology#hasPeriod
base:hasPeriod rdf:type owl:ObjectProperty ;
               rdfs:range base:Period .


###  http://purl.org/aiaontology#hasPoint
base:hasPoint rdf:type owl:ObjectProperty ;
              rdfs:domain base:Location .


###  http://purl.org/aiaontology#hasProvenance
base:hasProvenance rdf:type owl:ObjectProperty ;
                   rdfs:domain base:State ;
                   rdfs:range base:Activity .


###  http://purl.org/aiaontology#hasReputation
base:hasReputation rdf:type owl:ObjectProperty ;
                   rdfs:domain base:Agent ;
                   rdfs:range base:Reputation ;
                   rdfs:comment "EDIT"@en .


###  http://purl.org/aiaontology#hasRole
base:hasRole rdf:type owl:ObjectProperty ;
