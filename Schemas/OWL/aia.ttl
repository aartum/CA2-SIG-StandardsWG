             rdfs:range base:Location .


###  http://purl.org/aiaontology#hasVariable
base:hasVariable rdf:type owl:ObjectProperty .


###  http://purl.org/aiaontology#impacts
base:impacts rdf:type owl:ObjectProperty ;
             owl:inverseOf base:isImpactedBy ;
             rdfs:domain base:Activity ,
                         base:Event ;
             rdfs:range base:Environment .


###  http://purl.org/aiaontology#isDefinedBy
base:isDefinedBy rdf:type owl:ObjectProperty ;
                 rdfs:domain base:Environment ;
                 rdfs:range base:Parameter .


###  http://purl.org/aiaontology#isEnactedBy
base:isEnactedBy rdf:type owl:ObjectProperty ;
                 rdfs:domain base:Role ;
                 rdfs:range base:Agent .


###  http://purl.org/aiaontology#isImpactedBy
base:isImpactedBy rdf:type owl:ObjectProperty ;
                  rdfs:domain base:Environment ;
                  rdfs:range base:Activity ,
                             base:Event .


###  http://purl.org/aiaontology#isLimitedOrDirectedBy
base:isLimitedOrDirectedBy rdf:type owl:ObjectProperty ;
                           owl:inverseOf base:limitsOrDirects ;
                           rdfs:domain owl:Thing ;
                           rdfs:range base:Control .


###  http://purl.org/aiaontology#isOperatedBy
base:isOperatedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf base:operates ;
                  rdfs:domain base:Instrument ;
                  rdfs:range base:Operator .


###  http://purl.org/aiaontology#isOwnedBy
base:isOwnedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf base:owns ;
               rdfs:domain owl:Thing ;
               rdfs:range base:Owner .


###  http://purl.org/aiaontology#isParValOf
base:isParValOf rdf:type owl:ObjectProperty ,
                         owl:AsymmetricProperty ;
                rdfs:domain base:State ;
                rdfs:range base:Parameter .


###  http://purl.org/aiaontology#isPerformedBy
base:isPerformedBy rdf:type owl:ObjectProperty ;
                   rdfs:domain base:Activity ;
                   rdfs:range base:Agent .


###  http://purl.org/aiaontology#isStateDiffFrom
base:isStateDiffFrom rdf:type owl:ObjectProperty ;
                     rdfs:domain base:State ;
                     rdfs:range base:State .


###  http://purl.org/aiaontology#isStatementAbout
base:isStatementAbout rdf:type owl:ObjectProperty ;
                      rdfs:domain base:Claim ;
                      rdfs:range owl:Thing .


###  http://purl.org/aiaontology#isVerifiedBy
base:isVerifiedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf base:isVerifierOf ;
                  rdfs:domain base:Claim ;
                  rdfs:range base:Auditor .


###  http://purl.org/aiaontology#isVerifierOf
base:isVerifierOf rdf:type owl:ObjectProperty ;
                  rdfs:domain base:Auditor ;
                  rdfs:range base:Claim .


###  http://purl.org/aiaontology#limitsOrDirects
base:limitsOrDirects rdf:type owl:ObjectProperty ,
                              owl:AsymmetricProperty ;
                     rdfs:domain base:Control ;
                     rdfs:range owl:Thing .


###  http://purl.org/aiaontology#makesClaim
base:makesClaim rdf:type owl:ObjectProperty ;
                rdfs:domain base:Agent ;
                rdfs:range base:Claim ;
                rdfs:comment "FIX: this does not work while Role exists/Auditor etc. is not a subClassOf Agent."@en .


###  http://purl.org/aiaontology#makesImpactClaim
base:makesImpactClaim rdf:type owl:ObjectProperty ;
                      rdfs:domain base:Agent ;
                      rdfs:range base:ImpactClaim .


###  http://purl.org/aiaontology#operates
base:operates rdf:type owl:ObjectProperty ;
              rdfs:domain base:Operator ;
              rdfs:range base:Instrument .


###  http://purl.org/aiaontology#owns
base:owns rdf:type owl:ObjectProperty ;
          rdfs:domain base:Owner ;
          rdfs:range owl:Thing .


###  http://purl.org/aiaontology#performedWith
base:performedWith rdf:type owl:ObjectProperty ;
                   owl:inverseOf base:usedToPerform ;
                   rdfs:domain base:Activity ;
                   rdfs:range base:Instrument .


###  http://purl.org/aiaontology#usedToPerform
base:usedToPerform rdf:type owl:ObjectProperty ;
                   rdfs:domain base:Instrument ;
                   rdfs:range base:Activity .


###  http://purl.org/aiaontology#usesSymbol
base:usesSymbol rdf:type owl:ObjectProperty .


###  http://www.w3.org/2002/07/owl#equivalentClass
owl:equivalentClass rdf:type owl:ObjectProperty .


