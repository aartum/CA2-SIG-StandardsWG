#################################################################
#    Classes
#################################################################

###  http://purl.org/aiaontology#AccountingPeriod
base:AccountingPeriod rdf:type owl:Class ;
                      rdfs:subClassOf base:Period ;
                      rdfs:label "AccountingPeriod"@en .


###  http://purl.org/aiaontology#Activity
base:Activity rdf:type owl:Class ;
              owl:equivalentClass <http://www.w3.org/ns/prov#Activity> ;
              rdfs:subClassOf base:Event ;
              owl:disjointWith base:Event ;
              rdfs:comment "A thing that an agent does, which impacts an environment."@en ;
              skos:altLabel "Action"@en ;
              skos:definition "An event that is orchestrated by an agent."@en .


###  http://purl.org/aiaontology#Agent
base:Agent rdf:type owl:Class ;
           owl:equivalentClass terms:AgentClass ,
                               <http://www.w3.org/ns/prov#Agent> ;
           rdfs:comment "\"An agent is something that bears some form of responsibility for an activity taking place, for the existence of an entity, or for another agent's activity.\" (http://www.w3.org/ns/prov#Agent)"@en ;
           rdfs:isDefinedBy <http://www.w3.org/ns/prov#Agent> ;
           rdfs:label "Agent"@en ;
           rdfs:seeAlso <http://www.w3.org/ns/prov#Agent> ;
           skos:definition "A thing that bears some form of accountability for the occurrence of another thing."@en .


###  http://purl.org/aiaontology#Area
base:Area rdf:type owl:Class ;
          rdfs:subClassOf base:Extent ;
          rdfs:comment "TODO: definition" .


###  http://purl.org/aiaontology#Attestation
base:Attestation rdf:type owl:Class ;
                 rdfs:subClassOf base:Claim ;
                 rdfs:label "Attestation"@en ;
                 skos:definition "A formal statement made by an agent in which they claim to have witnessed or been presented with sufficient evidence to consider a specific claim made by another agent as truthful or accurate."@en .


###  http://purl.org/aiaontology#Attribute
base:Attribute rdf:type owl:Class ;
               rdfs:label "Attribute"@en ;
               skos:definition "A quality or feature of a thing that can be used, whether independently or in conjunction with other attributes, to identify it."@en .


###  http://purl.org/aiaontology#Auditing
base:Auditing rdf:type owl:Class ;
              rdfs:subClassOf base:Verifying ;
              rdfs:label "Auditing"@en ;
              skos:definition "The activity, performed by an auditor, of following a formal procedure to verify a claim."@en .


###  http://purl.org/aiaontology#Auditor
base:Auditor rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                          owl:onProperty base:compliesWithRequirements ;
                                                          owl:someValuesFrom base:Control
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty base:isVerifierOf ;
                                                          owl:someValuesFrom base:Claim
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf base:Verifier ;
             rdfs:label "Auditor"@en ;
             skos:definition "A predefined set of requirements, responsibilities and/or actions for an agent who conducts an audit."@en .

[ rdf:type owl:Axiom ;
   owl:annotatedSource base:Auditor ;
   owl:annotatedProperty owl:equivalentClass ;
   owl:annotatedTarget [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                owl:onProperty base:compliesWithRequirements ;
                                                owl:someValuesFrom base:Control
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty base:isVerifierOf ;
                                                owl:someValuesFrom base:Claim
                                              ]
                                            ) ;
                         rdf:type owl:Class
                       ] ;
   rdfs:comment "The requirement for an Agent to fulfill the Role of Auditor is that they verify some Claim AND complies with the (legal) Controls for being an Auditor"@en
 ] .


###  http://purl.org/aiaontology#Calculating
base:Calculating rdf:type owl:Class ;
                 rdfs:subClassOf base:Activity ;
                 rdfs:label "Calculating"@en ;
                 skos:definition "The activity of deriving the value of a parameter or indicator from one or more other values, according to some mathematical calculation formula."@en .


###  http://purl.org/aiaontology#CalculationFormula
base:CalculationFormula rdf:type owl:Class ;
                        rdfs:subClassOf base:Control ;
                        rdfs:label "Calculation Formula"@en ;
                        skos:definition "A control that guides a mathematical calculation."@en .


###  http://purl.org/aiaontology#Calibrator
base:Calibrator rdf:type owl:Class ;
