    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://purl.org/aiaontology#Operator"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2002/07/owl#equivalentClass"/>
        <owl:annotatedTarget>
            <owl:Restriction>
