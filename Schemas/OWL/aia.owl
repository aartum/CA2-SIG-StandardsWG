                <owl:someValuesFrom rdf:resource="http://purl.org/aiaontology#Instrument"/>
            </owl:Restriction>
        </owl:annotatedTarget>
        <rdfs:comment xml:lang="en">The requirement for an Agent to fulfill the Role of Operator is that they operate some Instrument.</rdfs:comment>
    </owl:Axiom>
    


