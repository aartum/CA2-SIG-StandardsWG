<?xml version="1.0"?>
<rdf:RDF xmlns="http://purl.org/aiaontology#"
     xml:base="http://purl.org/aiaontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://purl.org/aiaontology">
        <terms:description xml:lang="en">This ontology is a tool for aggregating and consolidating anthropogenic impact accounting data across different standards and vocabularies. The ontology is intended to be generic enough to be used for anthropogenic impact accounting in almost any discipline and context, including climate action impact accounting./s</terms:description>
        <terms:source rdf:resource="https://wiki.hyperledger.org/display/CASIG/An+ontology+for+anthropogenic+impact+accounting"/>
        <terms:title xml:lang="en">An Ontology for Anthropogenic Impact Accounting</terms:title>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description"/>
    


    <!-- http://purl.org/dc/terms/source -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/source"/>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#type -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
    


    <!-- http://www.w3.org/2002/07/owl#equivalentClass -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#equivalentClass"/>
    


    <!-- http://www.w3.org/2002/07/owl#equivalentProperty -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#equivalentProperty"/>
    


    <!-- http://www.w3.org/2002/07/owl#sameAs -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#sameAs"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#altLabel -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#altLabel"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#broader -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#broader"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#definition -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#definition"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#narrower -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#narrower"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#related -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#related"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dbpedia.org/ontology/person -->

    <rdfs:Datatype rdf:about="http://dbpedia.org/ontology/person"/>
    


    <!-- http://purl.org/dc/terms/date -->

    <rdfs:Datatype rdf:about="http://purl.org/dc/terms/date"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#Resource -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date">
        <owl:equivalentClass rdf:resource="http://purl.org/dc/terms/date"/>
    </rdfs:Datatype>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

   


    <!-- http://purl.org/aiaontology#isCalibratedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#isCalibratedBy">
        <owl:inverseOf rdf:resource="http://purl.org/aiaontology#calibrates"/>
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#MeasurementInstrument"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Calibrator"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#calibrates -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#calibrates">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Calibrator"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#MeasurementInstrument"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#compliesWithRequirements -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#compliesWithRequirements">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#comprises -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#comprises">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ReflexiveProperty"/>
        <rdfs:comment xml:lang="en">USE: For Activity, Control, Agent, State</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#defines -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#defines">
        <owl:inverseOf rdf:resource="http://purl.org/aiaontology#isDefinedBy"/>
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Parameter"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Environment"/>
        <rdfs:comment xml:lang="en">A collection of Parameters defines an Environment.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#enacts -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#enacts">
        <owl:inverseOf rdf:resource="http://purl.org/aiaontology#isEnactedBy"/>
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Agent"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Role"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#engagesIn -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#engagesIn">
        <owl:inverseOf rdf:resource="http://purl.org/aiaontology#isPerformedBy"/>
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Agent"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Activity"/>
    </owl:ObjectProperty>

    


    <!-- http://purl.org/aiaontology#hasCalculation -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasCalculation">
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Calculation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasClaimant -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasClaimant">
        <owl:inverseOf rdf:resource="http://purl.org/aiaontology#makesClaim"/>
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Claim(n)"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasCondition -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasCondition">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Control(n)"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Condition(n)"/>
        <rdfs:comment xml:lang="en">Indicates the State that must exist for the specific Control to be applicable.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasControl -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasControl">
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Control(n)"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasDuration -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasDuration">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Period"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#Duration"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasEnvironment -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasEnvironment">
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Environment"/>
        <rdfs:comment xml:lang="en">The Environment for which the Parameter&apos;s value was measured/calculated</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasFormula -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasFormula">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Calculation"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Formula"/>
    </owl:ObjectProperty>



    <!-- http://purl.org/aiaontology#hasInput -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasInput">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Activity"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Thing"/>
    </owl:ObjectProperty>

    


    <!-- http://purl.org/aiaontology#hasLocation -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasLocation">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Environment"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Location"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasMethodology -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasMethodology">
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Control(n)"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasOutput -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasOutput">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Activity"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Thing"/>
    </owl:ObjectProperty>



    <!-- http://purl.org/aiaontology#hasParVal -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasParVal">
        <owl:inverseOf rdf:resource="http://purl.org/aiaontology#isParValOf"/>
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Parameter"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#State"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasPeriod -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasPeriod">
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Period"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasPoint -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasPoint">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Location"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasProvenance -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasProvenance">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#State"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Activity"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Event"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasReputation -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasReputation">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Agent"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Reputation"/>
        <rdfs:comment xml:lang="en">EDIT</rdfs:comment>
    </owl:ObjectProperty>



    <!-- http://purl.org/aiaontology#hasSubject -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasSubject">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Control(n)"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasSymbol -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasSymbol"/>
    


    <!-- http://purl.org/aiaontology#hasType -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasType">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Location"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasVariable -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasVariable">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Calculation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#impacts -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#impacts">
        <owl:inverseOf rdf:resource="http://purl.org/aiaontology#isImpactedBy"/>
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Activity"/>
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Event"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Environment"/>
    </owl:ObjectProperty>


    <!-- http://purl.org/aiaontology#Impact(n) -->

    <owl:Class rdf:about="http://purl.org/aiaontology#Impact(n)">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Impact(n)</rdfs:label>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TODO: definition</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://purl.org/aiaontology#Parameter"/>
    </owl:Class>





    <!-- http://purl.org/aiaontology#isDefinedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#isDefinedBy">
