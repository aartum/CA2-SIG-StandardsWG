<?xml version="1.0"?>
<rdf:RDF xmlns="http://purl.org/aiaontology"
     xml:base="http://purl.org/aiaontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://purl.org/aiaontology">
        <terms:description xml:lang="en">The aim of this ontology is to provide a general and exhaustive standard for the accounting of human impact on their environment/s</terms:description>
        <terms:source rdf:resource="https://wiki.hyperledger.org/display/CASIG/An+ontology+for+anthropogenic+impact+accounting"/>
        <terms:title xml:lang="en">An Ontology for Anthropogenic Impact Accounting</terms:title>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description"/>
    


    <!-- http://purl.org/dc/terms/source -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/source"/>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#type -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
    


    <!-- http://www.w3.org/2002/07/owl#equivalentClass -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#equivalentClass"/>
    


    <!-- http://www.w3.org/2002/07/owl#equivalentProperty -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#equivalentProperty"/>
    


    <!-- http://www.w3.org/2002/07/owl#sameAs -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#sameAs"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#altLabel -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#altLabel"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#broader -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#broader"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#definition -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#definition"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#narrower -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#narrower"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#related -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#related"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dbpedia.org/ontology/person -->

    <rdfs:Datatype rdf:about="http://dbpedia.org/ontology/person"/>
    


    <!-- http://purl.org/dc/terms/date -->

    <rdfs:Datatype rdf:about="http://purl.org/dc/terms/date"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#Resource -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date">
        <owl:equivalentClass rdf:resource="http://purl.org/dc/terms/date"/>
    </rdfs:Datatype>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/aiaontology#activityImpacts -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#activityImpacts">
        <owl:inverseOf rdf:resource="http://purl.org/aiaontology#impactedByActivity"/>
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Activity"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Environment"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#comprises -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#comprises">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ReflexiveProperty"/>
        <rdfs:comment xml:lang="en">USE: For Activity, Control, Agent</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#enacts -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#enacts">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Agent"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Role"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#engagesIn -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#engagesIn">
        <owl:inverseOf rdf:resource="http://purl.org/aiaontology#isPerformedBy"/>
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Agent"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Activity"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#eventImpacts -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#eventImpacts">
        <owl:inverseOf rdf:resource="http://purl.org/aiaontology#impactedByEvent"/>
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Event"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#State"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#expressedInMedium -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#expressedInMedium">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Claim"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#ClaimMedium"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasClaimant -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasClaimant">
        <owl:inverseOf rdf:resource="http://purl.org/aiaontology#makesClaim"/>
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Claim"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasControl -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasControl">
        <owl:inverseOf rdf:resource="http://purl.org/aiaontology#hasSubject"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Control"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasDuration -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasDuration">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Period"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#Duration"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasInput -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasInput">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Activity"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasInstrument -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasInstrument">
        <owl:inverseOf rdf:resource="http://purl.org/aiaontology#usedToPerform"/>
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Activity"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Instrument"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasLocation -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasLocation">
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Location"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasOutput -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasOutput">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Activity"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasPeriod -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasPeriod">
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Period"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#hasStateDiff -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasStateDiff"/>
    


    <!-- http://purl.org/aiaontology#hasSubject -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#hasSubject">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Control"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#impactedByActivity -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#impactedByActivity">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Environment"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Activity"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#impactedByEvent -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#impactedByEvent">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#State"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Event"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#isDefinedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#isDefinedBy">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Environment"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Parameter"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#isPerformedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#isPerformedBy">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Activity"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#isStatementAbout -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#isStatementAbout">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Claim"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#makesClaim -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#makesClaim">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Agent"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Claim"/>
        <rdfs:comment xml:lang="en">FIX: this does not work while Role exists/Auditor etc. is not a subClassOf Agent.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/aiaontology#usedToPerform -->

    <owl:ObjectProperty rdf:about="http://purl.org/aiaontology#usedToPerform">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Instrument"/>
        <rdfs:range rdf:resource="http://purl.org/aiaontology#Activity"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/aiaontology#hasAccuracy -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#hasAccuracy">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#MeasurementInstrument"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/aiaontology#hasConfLowerBound -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#hasConfLowerBound">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#State"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/aiaontology#hasConfUpperBound -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#hasConfUpperBound">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#State"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/aiaontology#hasConfidenceLevel -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#hasConfidenceLevel">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#State"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/aiaontology#hasLabel -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#hasLabel">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">A optional human-understandable label for an entity of type Location, eg. a street adress.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/aiaontology#hasLogicalOperator -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#hasLogicalOperator">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Control"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Less than, greater than, equal to, include, exclude, etc.
Used to relate the subject and value of a Control, for example:

C a Control ;
   hasSubject ActivityB.startDate ;
   hasValue ActivityA.endDate ;
   hasOperator &gt;= .

is equivalent to the condition (Control) that Activity B only commence after the end of Activity A.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/aiaontology#hasMake -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#hasMake">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Instrument"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/aiaontology#hasModality -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#hasModality">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">Counterfactual (FALSE) or real (TRUE).</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/aiaontology#hasModel -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#hasModel">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Instrument"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/aiaontology#hasPrecision -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#hasPrecision">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#MeasurementInstrument"/>
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Parameter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/aiaontology#hasRationale -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#hasRationale">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Parameter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/aiaontology#hasTimestamp -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#hasTimestamp">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#State"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Format: YYYY-MM-DDThh:mm:ss</rdfs:comment>
        <rdfs:comment xml:lang="en">The moment of measurement of the value of a Parameter that defines the State.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/aiaontology#hasUnitOfMeasure -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#hasUnitOfMeasure">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Parameter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/aiaontology#hasValue -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#hasValue"/>
    


    <!-- http://purl.org/aiaontology#isBinding -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#isBinding">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Control"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">Used to indicate whether a Control is obligatory (TRUE) or non-obligatory (FALSE)</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/aiaontology#wasCalibratedOn -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#wasCalibratedOn">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#MeasurementInstrument"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Format: YYYY-MM-DDThh:mm:ss</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/aiaontology#wasManufacturedOn -->

    <owl:DatatypeProperty rdf:about="http://purl.org/aiaontology#wasManufacturedOn">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Instrument"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment xml:lang="en">Format: eg. 2022-01-16</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2006/time#hasDuration -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2006/time#hasDuration">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Period"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/prov#endedAtTime -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/prov#endedAtTime">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Period"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Format: YYYY-MM-DDThh:mm:ss</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/prov#startedAtTime -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/prov#startedAtTime">
        <rdfs:domain rdf:resource="http://purl.org/aiaontology#Period"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">Format: YYYY-MM-DDThh:mm:ss</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/aiaontology#AccountingPeriod -->

    <owl:Class rdf:about="http://purl.org/aiaontology#AccountingPeriod">
        <rdfs:subClassOf rdf:resource="http://purl.org/aiaontology#Period"/>
        <rdfs:label xml:lang="en">AccountingPeriod</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/aiaontology#Activity -->

    <owl:Class rdf:about="http://purl.org/aiaontology#Activity">
        <owl:equivalentClass rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/aiaontology#Event"/>
        <rdfs:comment xml:lang="en">A thing that an agent does, which impacts an environment.</rdfs:comment>
        <skos:altLabel xml:lang="en">Action</skos:altLabel>
        <skos:definition xml:lang="en">An event that is orchestrated by an agent.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/aiaontology#Agent -->

    <owl:Class rdf:about="http://purl.org/aiaontology#Agent">
        <owl:equivalentClass rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:comment xml:lang="en">&quot;An agent is something that bears some form of responsibility for an activity taking place, for the existence of an entity, or for another agent&apos;s activity.&quot; (http://www.w3.org/ns/prov#Agent)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:label xml:lang="en">Agent</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <skos:definition xml:lang="en">A thing that bears some form of accountability for the occurrence of another thing.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/aiaontology#Area -->

    <owl:Class rdf:about="http://purl.org/aiaontology#Area">
        <rdfs:subClassOf rdf:resource="http://purl.org/aiaontology#Extent"/>
        <rdfs:comment>TODO: definition</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/aiaontology#Attestation -->

    <owl:Class rdf:about="http://purl.org/aiaontology#Attestation">
        <rdfs:subClassOf rdf:resource="http://purl.org/aiaontology#Claim"/>
        <rdfs:label xml:lang="en">Attestation</rdfs:label>
        <skos:definition xml:lang="en">A formal statement made by an agent in which they claim to have witnessed or been presented with sufficient evidence to consider a specific claim made by another agent as truthful or accurate.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/aiaontology#Attribute -->

    <owl:Class rdf:about="http://purl.org/aiaontology#Attribute">
        <rdfs:label xml:lang="en">Attribute</rdfs:label>
        <skos:definition xml:lang="en">A quality or feature of a thing that can be used, whether independently or in conjunction with other attributes, to identify it.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/aiaontology#Auditing -->

    <owl:Class rdf:about="http://purl.org/aiaontology#Auditing">
        <rdfs:subClassOf rdf:resource="http://purl.org/aiaontology#Verifying"/>
        <rdfs:label xml:lang="en">Auditing</rdfs:label>
        <skos:definition xml:lang="en">The activity, performed by an auditor, of following a formal procedure to verify a claim.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/aiaontology#Auditor -->

    <owl:Class rdf:about="http://purl.org/aiaontology#Auditor">
        <rdfs:subClassOf rdf:resource="http://purl.org/aiaontology#Verifier"/>
        <rdfs:label xml:lang="en">Auditor</rdfs:label>
        <skos:definition xml:lang="en">A predefined set of requirements, responsibilities and/or actions for an agent who conducts an audit.</skos:definition>
    </owl:Class>
    


    <!-- http://purl.org/aiaontology#Calculating -->

    <owl:Class rdf:about="http://purl.org/aiaontology#Calculating">
